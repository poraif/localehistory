<table class="table is-fullwidth">
  <thead>
    <tr>
      <th>Title</th>
      <th>Category</th>
      <th>View</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    {{#each street.placemarks}}
      <tr>
        <td>
          {{title}}
        </td>
		<td>
          {{category}}
        </td>
		<td>
		  <a href="/street/{{../street._id}}/placemark/{{_id}}" class="ui icon button">
			<i class="fas fa-eye"></i>
		  </a>
		<td>
          <a href="/street/{{../street._id}}/editplacemark/{{_id}}" class="ui icon button">
            <i class="fas fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/street/{{../street._id}}/deleteplacemark/{{_id}}" class="ui icon button">
            <i class="fas fa-trash"></i>
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
